    <meta property="og:description" content="<%= ogDescription || 'Servidor FiveM de roleplay GTA V nuevo en desarrollo con rol serio, economía balanceada y comunidad activa en Latinoamérica y España.' %>" />
    <meta property="og:image" content="https://capinettarp.com.ar/assets/logo.png" />
    <meta property="og:image:width" content="756" />
    <meta property="og:image:height" content="756" />
    <meta property="og:url" content="https://capinettarp.com.ar<%= typeof url !== 'undefined' ? url : '/' %>" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:site" content="@CapiNettaRP" />
    <meta name="twitter:title" content="<%= title || 'Capi Netta RP' %>" />
    <meta name="twitter:description" content="<%= description || 'Servidor FiveM de roleplay GTA V nuevo en desarrollo. Abierto a Latinoamérica y España. Entra al Discord y prepárate para la apertura.' %>" />
    <meta name="twitter:image" content="https://capinettarp.com.ar/assets/logo.png" />
    <link rel="icon" type="image/png" href="/favicon.ico" />
    <link rel="shortcut icon" href="/favicon.ico" />
    <link rel="apple-touch-icon" href="/assets/logo-favicon.png" />
    <link rel="manifest" href="/manifest.json" />
    <link rel="sitemap" type="application/xml" href="/sitemap.xml" />
    <meta name="theme-color" content="#0b0b0f" />
    <meta name="msapplication-TileColor" content="#0b0b0f" />
    <title><%= title || 'Capi Netta RP | Domina La Ciudad' %></title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="preload" href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;700;900&family=Rajdhani:wght@300;500;700&display=swap" as="style" onload="this.onload=null;this.rel='stylesheet'">
    <noscript><link href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;700;900&family=Rajdhani:wght@300;500;700&display=swap" rel="stylesheet"></noscript>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" media="print" onload="this.media='all'">
    <link rel="stylesheet" href="/css/styles.css">
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Organization",
        "name": "Capi Netta RP",
        "description": "Servidor FiveM de roleplay GTA V en español con rol inmersivo, economía balanceada y comunidad activa para Latinoamérica y España.",
        "url": "https://capinettarp.com.ar",
        "logo": "https://capinettarp.com.ar/assets/logo.png",
        "image": "https://capinettarp.com.ar/assets/logo.png",
        "sameAs": [
            "https://discord.gg/tpxRFHugX7",
            "https://github.com/Capinetta-RP"
        ],
        "contactPoint": {
            "@type": "ContactPoint",
            "contactType": "customer support",
            "email": "contacto@capinettarp.com.ar",
            "availableLanguage": "es"
        },
        "areaServed": ["AR", "ES", "MX", "CO", "PE", "CL", "EC"],
        "address": {
            "@type": "PostalAddress",
            "addressCountry": "AR"
        }
    }
    </script>
</head>

<body>

    <!-- Header -->
    <header id="main-header" class="<%= headerClass || '' %>">
        <div class="container header-container">
            <a href="/" class="logo">
                <img src="/assets/logo.png" alt="Capi Netta RP Logo" loading="eager" decoding="async">
                <span>CAPI NETTA RP</span>
            </a>
            <button class="menu-toggle" aria-controls="primary-navigation" aria-expanded="false" aria-label="Abrir menú de navegación">
                <span class="sr-only">Abrir menú</span>
                <span class="menu-bar" aria-hidden="true"></span>
                <span class="menu-bar" aria-hidden="true"></span>
                <span class="menu-bar" aria-hidden="true"></span>
            </button>
            <nav aria-label="Navegación principal">
                <ul id="primary-navigation" class="nav-links">
                    <li><a href="/#info">INFORMACIÓN</a></li>
                    <li><a href="/#shop">TIENDA</a></li>
                    <li><a href="/#faq">FAQ</a></li>
                    <li><a href="/como-empezar" class="<%= page === 'como-empezar' ? 'active' : '' %>" <% if (page === 'como-empezar') { %>aria-current="page"<% } %>>CÓMO EMPEZAR</a></li>
                    <li><a href="/normativas-discord" class="<%= page === 'normativas-discord' ? 'active' : '' %>" <% if (page === 'normativas-discord') { %>aria-current="page"<% } %>>NORMATIVA DISCORD</a></li>
                    <li><a href="/normativas-gta" class="<%= page === 'normativas-gta' ? 'active' : '' %>" <% if (page === 'normativas-gta') { %>aria-current="page"<% } %>>NORMATIVA GTA</a></li>
                </ul>
            </nav>
            <span class="badge-pill" aria-label="Estado del servidor: nuevo en desarrollo">SERVIDOR NUEVO · EN DESARROLLO</span>
        </div>
    </header>
